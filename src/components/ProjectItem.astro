---
import { CardContainer, CardBody, CardItem } from '../components/ui/Card';

---

<CardContainer className="re-var" client:load>
    <CardBody className="bg-slate-950/70 relative group/card hover:shadow-2xl dark:hover:shadow-2xl hover:shadow-sky-400/[0.15] dark:hover:shadow-sky-400/[0.15] dark:bg-slate-950/70 dark:border-white/[0.2] border-white/[0.2] w-auto sm:w-[30rem] h-auto rounded-xl p-6 border">
      <CardItem
        translateZ="50"
        className="text-xl font-bold text-neutral-600 dark:text-white"
      >
        Sitio Web - Teknologix
      </CardItem>
      <CardItem
        as="p"
        translateZ="60"
        className="text-neutral-500 text-sm max-w-sm mt-2 dark:text-neutral-300"
      >
      Sitio Web de la empresa para mostrar información de Servicios, Soporte y Contacto.
      </CardItem>
      <CardItem translateZ="100" className="w-full mt-4">
        <img
          src="../../teknologix.jpg"
          height="1000"
          width="1000"
          class="h-60 w-full object-cover rounded-xl group-hover/card:shadow-xl"
          alt="thumbnail"
        />
      </CardItem>
      <div class="flex justify-between items-center mt-20">
        <CardItem
          translateZ={20}
          as="a"
          href="https://teknologix.mx" 
          target="_blank"
          className="px-4 py-2 rounded-xl text-base font-normal text-white/90 dark:text-white/90"
        >
          Ver →
        </CardItem>
        <CardItem
          translateZ={20}
          as="a"
          className="px-4 py-2 rounded-xl bg-black dark:bg-white dark:text-black text-white text-xs font-bold" href="https://teknologix.mx"
          target="_blank"
        >
          GitHub
        </CardItem>
      </div>
    </CardBody>
  </CardContainer>

  <CardContainer className="re-var" client:load>
    <CardBody className="bg-slate-950/70 relative group/card hover:shadow-2xl dark:hover:shadow-2xl hover:shadow-sky-400/[0.15] dark:hover:shadow-sky-400/[0.15] dark:bg-slate-950/70 dark:border-white/[0.2] border-white/[0.2] w-auto sm:w-[30rem] h-auto rounded-xl p-6 border">
      <CardItem
        translateZ="50"
        className="text-xl font-bold text-neutral-600 dark:text-white"
      >
        Sitio Web - RV Park 
      </CardItem>
      <CardItem
        as="p"
        translateZ="60"
        className="text-neutral-500 text-sm max-w-sm mt-2 dark:text-neutral-300"
      >
      Sitio Web del negocio para mostrar información de Servicios, Galería, Contacto, Ubicación.
      </CardItem>
      <CardItem translateZ="100" className="w-full mt-4">
        <img
          src="../../rvpark.jpg"
          height="1000"
          width="1000"
          class="h-60 w-full object-cover rounded-xl group-hover/card:shadow-xl"
          alt="thumbnail"
        />
      </CardItem>
      <div class="flex justify-between items-center mt-20">
        <CardItem
          translateZ={20}
          as="a"
          href="https://rvparkcampingsannicolas.com.mx" 
          target="_blank"
          className="px-4 py-2 rounded-xl text-base font-normal text-white/90 dark:text-white/90"
        >
          Ver →
        </CardItem>
        <CardItem
          translateZ={20}
          as="a"
          className="px-4 py-2 rounded-xl bg-black dark:bg-white dark:text-black text-white text-xs font-bold" href="https://rvparkcampingsannicolas.com.mx"
          target="_blank"
        >
          GitHub
        </CardItem>
      </div>
    </CardBody>
  </CardContainer>

